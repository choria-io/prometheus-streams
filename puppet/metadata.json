{
    "name": "choria-prometheus_streams",
    "version": "0.0.1",
    "author": "R.I.Pienaar <rip@devco.net>",
    "summary": "Install and configure a tool to federate Prometheus metrics via NATS Streams",
    "license": "Apache-2.0",
    "source": "https://github.com/choria-io/prometheus-streams",
    "issues_url": "https://github.com/choria-io/prometheus-streams/issues",
    "dependencies": [
        { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.12.0 < 5.0.0" }
    ],
